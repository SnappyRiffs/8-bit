module 8bit{
	uses local "74LS245" ifdef "74LS245"
	using "implements.h" unless ifdef "root";
	// similar to saying if root is defined don't define implements.h\
	root contains implements.h
	
	
	function clock (bool HLT, bool mode, int CLK, bool SS){
		if (!HLT){	
			if (mode){
				out=CLK;
			}
			else{
				out=SS;
			}
		}
	}
}
