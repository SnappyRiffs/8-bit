module 8bit{
	using local "74LS245" // similar to `import 74LS245`
	using local "implements.h"
	// bool and int are similar and thus interchangable
	
	function clock (bool HLT, bool mode, int CLK, bool SS){
		int out = 0;
		if (!HLT){	
			if (mode){
				out = CLK;
			}
			else{
				out = (int) SS;
			}
		}
	}
	function register (bool invAI, bool invAO, bool CLK, bool CLR, int[8] BUS){
		int[8]
	}
}
